{
    "name": "guanguans/laupload",
    "description": "一个 laravel 上传小组件",
    "type": "library",
    "keywords": ["laravel", "upload", "laupload"],
    "homepage": "https://github.com/guanguans/laupload",
    "require": {
        "php": ">=5.4.0",
        "npm-asset/hdjs": "master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "guanguans",
            "email": "798314049@qq.com"
        }
    ],
    "config": {
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library": "public/assets/node_modules"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "guanguans\\laupload\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "guanguans\\laupload\\LauploadServiceProvider"
            ],
            "aliases": {
                "laupload": "guanguans\\laupload\\laupload"
            }

        }
    }
}
